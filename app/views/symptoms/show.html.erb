<div class="container">
  <div class="col-md-offset-4 col-md-4">
    <div class="panel panel-default">
      <div class="panel-body">
        <p>
          <h3>Title:</h3>
          <%= @symptom.title %>
        </p>
          <%= link_to image_tag(@symptom.image.url(:med)), @symptom.image.url %>
        <p>
          <h4>Text:</h4>
          <%= @symptom.text %>
        </p>

      </div>
      <div class="panel-footer">
        <%= link_to 'Back', crop_symptoms_path %>
        | <%= link_to 'Edit', edit_crop_symptom_path(@crop,@symptom) %>
      </div>
    </div>
  </div>
</div>