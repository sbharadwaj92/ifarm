<div id="crops" class="transitions-enabled">
  <% @crop.symptoms.each do |symptom| %>
    <div class="box panel panel-default">
      
      <%= link_to image_tag(symptom.image.url(:med)), symptom.image.url %>

      <div class="panel-body">

          <%= link_to 'Show', crop_symptom_path(@crop,symptom) %>
          <%= link_to 'Edit', edit_crop_symptom_path(@crop,symptom) %>
          <%= link_to "Destroy", [symptom.crop,symptom],
                          method: :delete, data: { confirm: 'Are you sure?' } %>

      </div>
    </div>
  <% end %>
</div>

<div class="center">
  <%= will_paginate @posts, renderer: BootstrapPagination::Rails %>
</div>